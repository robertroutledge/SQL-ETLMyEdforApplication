        public void Main()
        {
            //builds a list of the GDE file names, may not need it
            //List<string> GDE_Filenames = new List<string> { "StudentPeriodAbsencesAttend.txt", "TeacherCourse.txt", 
            //    "UserSecurityInformation.txt", "StudentDemographicEnh.txt", "StudentPrograms.txt", "StudentDemographic.txt", 
            //   "StudentDailyAbsences.txt", "StudentCourseSelection.txt", "StudentCourseHistory.txt", "StaffInformation.txt", 
            //   "SchoolDetails.txt", "SchoolRooms.txt", "StaffHomerooms.txt", "ParentInformation.txt", "MedicalInformation.txt", "NonSchoolDays.txt", "HomeroomAndStaffAssignments.txt", "IncidentInformation.txt", "StudentPeriodAbsencesEnh.txt", "", "", "", "" };



            //Sets working directory, builds a list of the .txt files in it
            var location = @"c:\scripts\download\";
            string[] files = Directory.GetFiles(location, "*.txt");

            //var location = @"c:\";
            //string[] files = Directory.GetFiles("CourseMarks.txt");

            //Opens each txt file and cleans it
            foreach (string file in files)
            {
                string filename = Path.GetFileName(file);
                var temp_fileContent = File.ReadAllLines(file);
                //creates the strings that can cause problems with commas, and what to replace them with
                //replaced commas with |
                //removes double quotes
                //Excludes text between two double quotes
                string pipe = @"|";

                //p1 and p2 are used to build pattern_one, to get around pipe operator
                string p0 = @"[|]";
                string p1 = "(,)(?=(?:[^\"]";
                string p2 = "\"[^\"]*\")*$)";
                string pattern_one = p1 + pipe + p2;
                string pattern_two = "\"\\" + pipe;
                string pattern_three = "\\" + pipe + "\"";

                //Does the replacing above
                temp_fileContent = temp_fileContent.Select(x => new Regex(p0).Replace(x, "")).ToArray();
                temp_fileContent = temp_fileContent.Select(x => new Regex(pattern_one).Replace(x, pipe)).ToArray();
                temp_fileContent = temp_fileContent.Select(x => new Regex(pattern_two).Replace(x, pipe)).ToArray();
                temp_fileContent = temp_fileContent.Select(x => new Regex(pattern_three).Replace(x, pipe)).ToArray();
                temp_fileContent = temp_fileContent.Select(x => new Regex("\"").Replace(x, "")).ToArray();

                //Saves the file in a new directory, with _cleaned
                int index = filename.IndexOf(".");
                if (index < 1)
                    filename = filename + "_cleaned.txt";
                filename = filename.Substring(0, index) + "_cleaned.txt";
                string save_location = location + @"SSIS_Cleaned\";
                File.WriteAllLines(save_location + filename, temp_fileContent);
            }


            Dts.TaskResult = (int)ScriptResults.Success;
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

    }
}]]></ProjectItem>
